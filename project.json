{
  "name": "mami-delice",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mami-delice/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{workspaceRoot}/dist/mami-delice"],
      "options": {
        "outputPath": "dist/mami-delice"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "port": 4200
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "port": 4300
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/mami-delice"],
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
